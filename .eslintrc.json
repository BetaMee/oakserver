{
  "env": {
      "commonjs": true,
      "es6": true,
      "node": true
  },
  "globals": {
      "graphql": true
  },
  "extends": [                                                                                                             
      "eslint:recommended"                                                                                                        
    ],
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true
      },
      "sourceType": "module"
  },
  "plugins": [
  ],
  "rules": {
      "indent": ["error",2],
      "linebreak-style": ["error", "windows"],
      "quotes": ["error", "single"],
      "semi": ["error", "never"],
      "no-console": "warn",
      "no-unused-vars": "warn"
  }
}